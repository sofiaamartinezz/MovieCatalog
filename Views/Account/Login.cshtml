@model MovieCatalog.Models.LoginDTO
@namespace MovieCatalog.Views.Account
@{
    <form method="post">

        <div>
            <label>Email</label>
            <input asp-for="Email" />
        </div>

        <div>
            <label>Password</label>
            <input asp-for="Password" type="password" />
        </div>

        <button type="submit"> Login</button>
    </form>
}